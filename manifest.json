{
  "name": "WATimezones",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "See your friends' local times in Web Whatsapp.",
  "homepage_url": "https://github.com/smddzcy/watimezones",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "storage",
    "https://web.whatsapp.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}